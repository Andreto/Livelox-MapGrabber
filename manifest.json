{
  "manifest_version": 3,
  "name": "Livelox Map Data Grabber",
  "version": "1.0.0",
  "description": "Extract map data from Livelox.",
  "permissions": [
    "webRequest",
    "storage"
  ],
  "host_permissions": [
    "https://www.livelox.com/Viewer/*",
    "https://livelox.blob.core.windows.net/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "Livelox Data Grabber"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
